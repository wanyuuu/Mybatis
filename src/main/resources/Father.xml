<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wanyu.Mapper.IFatherDao">
    <insert id="addFather" parameterType="com.wanyu.model.Father">
        INSERT INTO FATHER(name,age) VALUES (#{name},#{age})
    </insert>
    <select id="getFatherList" resultType="com.wanyu.model.Father">
        SELECT *FROM FATHER
    </select>
</mapper>